{
  "name": "plank",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:app": "vite",
    "dev:server": "vite build --mode development && wrangler dev --local",
    "dev:server:watch": "vite build --mode development --watch",
    "dev:server:start": "wrangler dev --local",
